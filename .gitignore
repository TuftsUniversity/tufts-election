config/fedora.yml
config/initializers/secret_token.rb
config/database.yml
config/solr.yml
config/jetty.yml
config/devise.yml

# ignore utils directory only used on dev
/utils/*

#ignore the office authority file
#config/office-authority.xml

# ignore precompiled assets
public/assets

# ignore ide files
.idea/
