          <aas:result elector_ref="2" resp="Philip Lampi" vote="43"></aas:result>
          <aas:result elector_ref="3" resp="Philip Lampi" vote="38"></aas:result>
          <aas:result elector_ref="4" resp="Philip Lampi" vote="20"></aas:result>
        </aas:sub_unit>
        <aas:sub_unit geog_id="null" name="Senate" type="null">
          <aas:result elector_ref="1" resp="Philip Lampi" vote="18"></aas:result>
          <aas:result elector_ref="2" resp="Philip Lampi" vote="19"></aas:result>
          <aas:result elector_ref="3" resp="Philip Lampi" vote="18"></aas:result>
          <aas:result elector_ref="4" resp="Philip Lampi" vote="11"></aas:result>
        </aas:sub_unit>
      </aas:admin_unit>
    </aas:role>
  </aas:office>
  <aas:reference resp="Philip Lampi" type="citation">2nd Session, House of Representatives Journal, pp. 47-48, Alabama Archives, Montgomery.</aas:reference>
  <aas:reference resp="Philip Lampi" type="citation">2nd Session (1820), House of Representatives Journal (or Journal of the House of Representatives of the State of Alabama), pp. 47-48, Alabama Archives, Montgomery.</aas:reference>
