<foxml:xmlContent>
<aas:election_record xmlns:aas="http://dca.tufts.edu/aas" xmlns="http://www.fedora.info/definitions/" xmlns:xlink="http://www.w3.org/1999/xlink" label="Alabama 1820 Presidential Electors" type="Legislative" iteration="First Ballot" date="1820-11" election_id="al.presidentialelectors.1820" handle="10427/55774" schemaLocation="http://dca.tufts.edu/aas http://dca.tufts.edu/schema/aas/electionRecord.xsd">
               <aas:office name="Electoral College" office_id="ON050" scope="Federal">
	                 <aas:role title="Elector" scope="State">
		                   <aas:ballot>
			                     <aas:elector name="John Scott" name_id="null" affiliation="null" affiliation_id="null" declared_for="null" elector_num="1"></aas:elector>
			                     <aas:elector name="Henry Minor" name_id="null" affiliation="null" affiliation_id="null" declared_for="null" elector_num="2"></aas:elector>
			                     <aas:elector name="George W. Philips" name_id="null" affiliation="null" affiliation_id="null" declared_for="null" elector_num="3"></aas:elector>
			                     <aas:elector name="James S. Walker" name_id="null" affiliation="null" affiliation_id="null" declared_for="null" elector_num="4"></aas:elector>
			                     <aas:note resp="Philip Lampi">The source documents do not say if the electors were pledged to vote for a specific presidential candidate.</aas:note>
		                   </aas:ballot>
		                   <aas:overview status="Version 1.0" modified="2007-05-17">
			                     <aas:elector_summary elector_ref="1" vote_total="56" resp="Philip Lampi"></aas:elector_summary>
			                     <aas:elector_summary elector_ref="2" vote_total="62" resp="Philip Lampi"></aas:elector_summary>
			                     <aas:elector_summary elector_ref="3" vote_total="63" resp="Philip Lampi"></aas:elector_summary>
			                     <aas:elector_summary elector_ref="4" vote_total="31" resp="Philip Lampi"></aas:elector_summary>
			                     <aas:note resp="Philip Lampi">&quot;If George W. Owen (Speaker of the House of Representatives) voted for Henry Minor, - a point implied but not revealed in the House Journal ballot - then Henry Minor&apos;s total in the House ballot is 44 votes.  His final tally is then 63, a point emphasized by the House ballot.&quot;</aas:note>
		                   </aas:overview>
		                   <aas:admin_unit type="State" name="Alabama" geog_id="als">
			                     <aas:sub_unit type="null" name="House of Representatives" geog_id="null">
				                       <aas:result elector_ref="1" vote="38" resp="Philip Lampi"></aas:result>
				                       <aas:result elector_ref="2" vote="43" resp="Philip Lampi"></aas:result>
				                       <aas:result elector_ref="3" vote="45" resp="Philip Lampi"></aas:result>
				                       <aas:result elector_ref="4" vote="20" resp="Philip Lampi"></aas:result>
			                     </aas:sub_unit>
			                     <aas:sub_unit type="null" name="Senate" geog_id="null">
				                       <aas:result elector_ref="1" vote="18" resp="Philip Lampi"></aas:result>
				                       <aas:result elector_ref="2" vote="19" resp="Philip Lampi"></aas:result>
				                       <aas:result elector_ref="3" vote="18" resp="Philip Lampi"></aas:result>
				                       <aas:result elector_ref="4" vote="11" resp="Philip Lampi"></aas:result>
			                     </aas:sub_unit>
		                   </aas:admin_unit>
	                 </aas:role>
               </aas:office>
	              <aas:reference type="citation" resp="Philip Lampi">2nd Session, House of Representatives Journal, pp. 47-48, Alabama Archives, Montgomery.</aas:reference>
	              <aas:reference resp="Philip Lampi" type="citation">2nd Session (1820), House of Representatives Journal (or Journal of the House of Representatives of the State of Alabama), pp. 47-48, Alabama Archives, Montgomery.</aas:reference>
            </aas:election_record>
</foxml:xmlContent>
