<aas:election_record xmlns:aas="http://dca.tufts.edu/aas" date="1820-11" election_id="al.presidentialelectors.1820" handle="10427/55774" iteration="First Ballot" label="Alabama 1820 Presidential Electors" schemaLocation="http://dca.tufts.edu/aas http://dca.tufts.edu/schema/aas/electionRecord.xsd" type="Legislative">
  <aas:office name="Electoral College" office_id="ON050" scope="Federal">
