<table id="browse-boxes-table">
  <tr>
    <td>
      <div class="browse-box text1">
        <p> <img alt="By Candidate" src="/assets/subtitle_bycandidate.gif" /> </p>
        <p>View complete list of candidates and links to their elections.</p>
        <div class="browse-submit">
          <form action="/catalog/facet/candidate_name_facet" method="get">
            <p>
              <select name="facet.prefix">
                <option value="A">A</option>
                <option value="B">B</option>
                <option value="C">C</option>
                <option value="D">D</option>
                <option value="E">E</option>
                <option value="F">F</option>
                <option value="G">G</option>
                <option value="H">H</option>
                <option value="I">I</option>
                <option value="J">J</option>
                <option value="K">K</option>
                <option value="L">L</option>
                <option value="M">M</option>
                <option value="N">N</option>
                <option value="O">O</option>
                <option value="P">P</option>
                <option value="Q">Q</option>
                <option value="R">R</option>
                <option value="S">S</option>
                <option value="T">T</option>
                <option value="U">U</option>
                <option value="V">V</option>
                <option value="W">W</option>
                <option value="X">X</option>
                <option value="Y">Y</option>
                <option value="Z">Z</option>
              </select>
              <input type="image" alt="go" src="/assets/button_arrow.gif" />
            </p>
          </form>
        </div>
      </div>
    </td>
    <td>
      <div class="browse-box text1">
        <p> <img alt="By Office" src="/assets/subtitle_byoffice.gif" /> </p>
        <p>View list of elections by the political office.</p>
        <div class="browse-submit">
          <p>
            <a href="office-list.xq"> <img alt="go" src="/assets/button_arrow.gif" /> </a>
          </p>
        </div>
      </div>
    </td>
  </tr>
  <tr>
    <td>
      <div class="browse-box text1">
        <p> <img alt="By Year" src="/assets/subtitle_byyear.gif" /> </p>
        <p>View all the elections available by year.</p>
        <div class="browse-submit">
          <form action="year-list.xq" method="get">
            <p>
              <select name="browse-year"> <%= render :partial=>'shared/year_options' %> </select>
              <input type="image" alt="go" src="/assets/button_arrow.gif" />
            </p>
          </form>
        </div>
      </div>
    </td>
    <td>
      <div class="browse-box text1">
        <p> <img alt="By State" src="/assets/subtitle_bystate.gif" /> </p>
        <p>View a list of elections by state.</p>
        <div class="browse-submit">
          <form action="state-list.xq" method="get">
            <p>
              <select name="browse-state"> <%= render :partial=>'shared/state_options' %> </select>
              <input type="image" alt="go" src="/assets/button_arrow.gif" />
            </p>
          </form>
        </div>
      </div>
    </td>
  </tr>
</table>